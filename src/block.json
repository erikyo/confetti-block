{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "blocchi/confetti",
  "version": "0.1.0",
  "title": "Confetti",
  "category": "widgets",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    }
  },
  "example": {
    "attributes": {
      "content": "Hello world"
    }
  },
  "textdomain": "block-development-examples",
  "editorScript": "file:./index.js",
  "viewScript": "file:./script.js",
  "viewStyle": "file:./style-script.css",
  "supports": {
    "color": {
      "text": true,
      "link": true,
      "background": true
    }
  }
}